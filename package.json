{
  "name": "mec",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run lint -- --fix && vue-cli-service serve",
    "build": "npm run lint -- --fix && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "eslint": "eslint -f checkstyle --ext .js,.vue src -o eslint_result.xml"
  },
  "dependencies": {
    "a": "^2.1.2",
    "area-data-vue": "^1.0.9",
    "area-linkage-vue": "^5.1.2",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "echarts": "^4.1.0",
    "echarts-china-cities-js": "^0.1.1",
    "el-table-pagination": "^0.4.38",
    "element-china-area-data": "^4.1.2",
    "element-ui": "^2.13.2",
    "elliptic": "^6.5.3",
    "http-proxy-agent": "^4.0.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.19",
    "mavon-editor": "^2.9.0",
    "sass-loader": "^8.0.2",
    "serialize-javascript": "^3.1.0",
    "v-charts": "^1.19.0",
    "vue": "^2.6.11",
    "vue-amap": "^0.5.10",
    "vue-generate-component": "^2.1.1",
    "vue-i18n": "^8.17.7",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "less": "^3.11.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/base",
      "plugin:vue/essential",
      "plugin:vue/strongly-recommended",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
